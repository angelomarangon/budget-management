@model ResumenMontoViewModel

<div class="contenedor-totales mb-3">
    <div>
        <div>
            <strong>Depositos</strong>
        </div>
        <div id="activo">
            @Model.Ingresos.ToString("N")
        </div>
    </div>

    <div>
        <div>
            <strong>Retiros</strong>
        </div>
        <div id="pasivo">
            @Model.Gastos.ToString("N")
        </div>
    </div>

    <div>
        <div>
            <strong>Total</strong>
        </div>
        <div>
            @Model.Total.ToString("N")
        </div>
    </div>
</div>